{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "es2015",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "allowJs": true
  },
"include": [
  "./src/",
  "declaration.d.ts"
  ]
}